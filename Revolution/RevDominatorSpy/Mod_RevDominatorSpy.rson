              "              vHour=Rnd(0,23);",
              "              vMin=Rnd(0,59);",
              "              vSec=Rnd(0,59);",
              "              if(vZondMemory>=96) { //Вывод уведомление о возвращении на базу",
              "                Ether(2,'008DB316-DEE1-4BE1-A6BF-875942EC3671',\"<GameDateTxtByTurn(CurTurn())><br><vHour>:<vMin>:<vSec><br>Память зонда заполнена! Пора возвращаться на научную базу.\");",
              "              }",
              "              if(vZondMemory<96) { //Вывод уведомления о прогрессе сбора информации",
              "                Ether(2,'008DB316-DEE1-4BE1-A6BF-875942EC3671',\"<GameDateTxtByTurn(CurTurn())><br><vHour>:<vMin>:<vSec><br>Зонд записал информации на <vZondMemory>% памяти.<br>Свободно: <(100-vZondMemory)>%\");",
              "                vZondDaysPl=vZondDaysPl+1;",
              "                if(vStateDom<4) vZondMemory=vZondMemory+3;",
              "              }",
              "            }",
              "          }",
              "        }",
              "      }",
              "    }",
              "  }",
              "}",
              "if(vZondDaysPl>=30) { //Если зонд собирал информацию 30 дней",
